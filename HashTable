public class HashTable {
    Stack[] table; // 
    
    
    public HashTable(int size) {
      
     table = new Stack[size];           //initialize the table array with empty Stack objects// each element of the array, create a 5 Stack object and assign them to each array eleme
                                        // for each element of the array, create a 5 Stack object and assign them to each array element.
  for(int i=0;i<table.length;i++){
   table[i]= new Stack(); 
 
  }
    }                            
    
    public void insert(Node n) {
                                //determine the hash key of Node n
                                //using the key to determine the table location, 
                                //push Node n onto the stack
    }
    public void print() {
       
		//display the contents of the entire table in order
        for (int i=0; i < table.length; i++) {
            System.out.println("===== Position " + i + " ======\n");
            table[i].print();
            System.out.println("========= End ==========");
            System.out.println();
        }
        
    }
}
